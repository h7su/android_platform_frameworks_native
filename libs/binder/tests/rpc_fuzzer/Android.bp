    static_libs: [
    "libprocessgroup",
        "libbase",
        "libcutils",
        "libjsoncpp",
        "liblog",
        "libutils",
        "libbinder",
    ],
    target: { android: { shared_libs: ["libvndksupport"], }, },
}
